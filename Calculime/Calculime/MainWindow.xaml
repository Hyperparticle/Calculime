<Window x:Class="Calculime.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Calculime" Height="350" Width="500" Icon="Resources/favicon.ico">

    <StackPanel Margin="20">
        <TextBox Height="23" x:Name="InputTextBox" TextWrapping="Wrap" Text="Input Here"/>
        <Button Content="Calculate" x:Name="CalculateButton" Click="CalculateButton_Click"/>
        <TextBlock x:Name="OutputTextBlock" TextWrapping="Wrap" Text="Output Here" Height="63"/>
    </StackPanel>
</Window>
